{
  "name": "edusocial-backend",
  "version": "1.0.0",
  "description": "Aplicativo escolar com a finalidade de aumentar a participação dos responsáveis no desenvolvimento dos alunos da educação infantil",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --exit-child index.ts",
    "build": "yarn tsc",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "migrate": "npx prisma migrate dev",
    "migrate-deploy": "npx prisma migrate deploy"
  },
  "author": "Felipe Vieira Sobral <xfelipesobral@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "bcrypt": "^5.1.0",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.5.0",
    "prisma": "^4.10.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
